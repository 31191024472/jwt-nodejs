<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Updat user</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
</head>

<body>
    <div class="user-container">
    <form action="/user/update-user" method="post">
         <div class="container ">
                <div class="form-create-user mt-3 mb-3">
                    <h1>Update Users</h1>
                    <input hidden value="<%= userData.id %>" name =  "id">
                    <div class="mb-3 col-6 ">
                        <label for="username" class="form-label">User Name</label>
                        <input value="<%= userData.username %>"  type="text" class="form-control" id="username" name="username">
                    </div>
                    <div class="mb-3 col-6">
                        <label for="email" class="form-label">Email address</label>
                        <input value="<%= userData.email%>" type="email" class="form-control" id="email" name="email" >
                    </div>
                    <button type="submit" class="btn btn-primary">Update</button>
                </div> 
        
                    
            </div>
    </form>
    </div>
  
</body>

</html>